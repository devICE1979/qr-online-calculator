json
{
  "version": 2,
  "builds": [
    {
      "src": "api/calculate.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/calculate",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/calculate.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
